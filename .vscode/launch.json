{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run [DEV] Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}"
			],
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"preLaunchTask": "${defaultBuildTask}",
			"env": {
				"ENV": "dev",
			}
		}
	]
}